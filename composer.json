{
    "name": "instalution/activitypub",
    "type": "library",
    "version": "0.6.0",
    "description": "Fork of `landrok/activitypub` @ https://github.com/landrok/activitypub",
    "keywords": ["ActivityPub", "ActivityStreams", "federation"],
    "homepage": "https://github.com/instalution/activitypub",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/cache": "^6.3",
        "psr/cache": "^3.0",
        "phpseclib/phpseclib": "^3.0",
        "monolog/monolog": "^3.4",
        "symfony/http-foundation": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ActivityPhp\\": "src/ActivityPhp/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActivityPhpTest\\": "tests/ActivityPhp/"
        }
    },
    "archive": {
        "exclude": ["/docs"]
    }
}
